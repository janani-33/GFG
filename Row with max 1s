class Solution {
    int rowWithMax1s(int arr[][], int n, int m) {
        int max=0,maxindex=-1;
        for(int i=0;i<n;i++){
            int c=0;
            for(int j=0;j<m;j++){
                if(arr[i][j]==1)
                c++;
            }
            if(c>max){
                max=c;
                maxindex=i;
            }
        }
        return maxindex;
    }
}
