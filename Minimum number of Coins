class Solution{
    static List<Integer> minPartition(int N)
    {
        int a[]={1,2,5,10,20,50,100,200,500,2000};
        List<Integer> res=new ArrayList<>();
        for(int i=a.length-1;i>=0;i--){
            if(N==0)
            break;
            while(a[i]<=N){
                N-=a[i];
                res.add(a[i]);
            }
        }
        return res;
    }
}
