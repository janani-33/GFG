class Solution {
    public ArrayList<Integer> Ancestors(Node root, int target) {
        ArrayList<Integer> list=new ArrayList<>();
        findancestors(root,target,list);
        return list;
    }
    public boolean findancestors(Node root,int target,ArrayList<Integer> list){
        if(root==null)
        return false;
        if(root.data==target)
        return true;
        boolean l=findancestors(root.left,target,list);
        boolean r=findancestors(root.right,target,list);
        if(l||r)
        list.add(root.data);
        return l||r;
    }
}
