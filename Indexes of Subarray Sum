class Solution {
    static ArrayList<Integer> subarraySum(int[] arr, int n, int s) {
        int j=0,sum=0;
        for(int i=0;i<n;i++){
            sum+=arr[i];
            while(sum>s && j<i)
            sum-=arr[j++];
            if(sum==s)
            return new ArrayList<Integer>(Arrays.asList(j+1,i+1));
        }
        return new ArrayList<Integer>(Arrays.asList(-1));
    }
}
