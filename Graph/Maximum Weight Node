class Solution{
    public int maxWeightCell(int N, int Edge[]){
        int max=Integer.MIN_VALUE,index=-1;
        int arr[]=new int[N];
        for(int i=0;i<N;i++){
            if(Edge[i]!=-1)
            arr[Edge[i]]=arr[Edge[i]]+i;
        }
        for(int i=0;i<N;i++){
            if(arr[i]>=max){
                max=arr[i];
                index=i;
            }
        }
        return index;
    }
}
