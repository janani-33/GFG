/* A Binary Tree node
class Node
{
    int data;
    Node left, right;

    Node(int item)
    {
        data = item;
        left = right = null;
    }
}*/

class Solution
{
	Node lca(Node root, int n1,int n2)
	{
	    if(root==null)
	    return null;
	    if(root.data==n1 || root.data==n2)
	    return root;
	    Node lt=lca(root.left,n1,n2);
	    Node rt=lca(root.right,n1,n2);
	    if(lt!=null && rt!=null)
	    return root;
	    if(lt==null)
	    return rt;
	    else
	    return lt;
	}
}
