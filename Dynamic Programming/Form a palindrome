class Solution{
    static int countMin(String str)
    {
        int n=str.length();
        int dp[][]=new int[n][n];
        for(int i=1;i<n;i++){
            for(int j=0,k=i;k<n;j++,k++)
            dp[j][k]=(str.charAt(j)==str.charAt(k))?dp[j+1][k-1]:(Math.min(dp[j][k-1],dp[j+1][k])+1);
        }
        return dp[0][n-1];
    }
}
