//function Template for Java
/* linked list node class:
class Node {
    int data;
    Node next;
    Node(int value) {
        this.value = value;
    }
}
*/

class Solution
{
    //Function to reverse a linked list.
    Node reverseList(Node head)
    {
        // code here
        if(head==null)
        return null;
        Node temp=head;
        Node temp2=head.next;
        head.next=null;
        while(temp2!=null){
            Node t=temp2.next;
            temp2.next=temp;
            temp=temp2;
            temp2=t;
        }
        return temp;
    }
}
